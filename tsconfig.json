{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "ES6",
    "module": "ES6",
    "noImplicitAny": true,
    "allowJs": true,
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true
  }
}
